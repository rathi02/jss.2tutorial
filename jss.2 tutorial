//1
function isPrime(num){
  for (var i=2; i < num; i++){
    if (num % i==0){
      return false;
    }
  }
  return true;
}

console.log(isPrime(23));
console.log(isPrime(39));
console.log(isPrime(101));
for (var i = 2; i<1000; i++){
  if (isPrime(i) ==true){
    console.log(i);
  }
}

//2

var num1 = 1;
var num2= 1;
for (var i = 0; i < 10; i++){
  console.log(num1);
  console.log(num2);
  num1 = num1 +num2;
  num2= num2 + num1;
}

//3
function reverseString (str){
  var answer = "";
  for (var i = str.legth-1; i>=0; i--){
    answer += str.charAt(i);
  }
  return answer;
}
console.log(reverseString("hello"));
console.log(reverseString("I am happy"));

//4
function largestofFour(arr){
  var answers = [];
  
  for (var i = 0; i < 4; i++){
    var max = arr [i] [0];
    for (var j = 0; j < 4; j++){
      if (arr [i] [j]);
    }
  }
  answers.push(max);
}
return answers;
}

largestOfFour([4,5,1,3], [13,27,18,26], [32,35,37,39],[1000,1001,857,1]);


